prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
freetypelibs=
mathlibs=-lm
zlibs=-lz
freetypecflags=

Name: Xfont
Description: X font Library
Version: 1.4.2
Cflags: -I${includedir} -I${includedir}/X11/fonts ${freetypecflags}
Libs: -L${libdir} -lXfont ${freetypelibs} ${mathlibs} ${zlibs}
